import java.util.Random;
public class Main {

	public static void main(String[] args) {
		Random r = new Random();
		for(int i = 1; i<20;i++) 
		{
			int l = r.nextInt(1);
			int p = 0;
			float prumer = 0;
			if(l == 1) {
			p = r.nextInt(101)*(-1);
			}
			else 
			{
				p = r.nextInt(101);
			}
			System.out.println("cislo "+i+": "+p);
			prumer = prumer + p / i;
		}
		
		
		
		
		
		
	}

}
